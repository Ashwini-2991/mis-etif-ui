{
    "name": "angular-starter-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "build:dev": "ng build --aot --configuration development",
        "build:prod": "ng build --aot --configuration production",
        "test": "ng test --code-coverage",
        "test:ci": "ng test --browsers=ChromeHeadlessCI --watch=false --code-coverage",
        "test:coverage": "concurrently \"npm test\" \"live-server --port=4300 coverage\"",
        "stats": "npm run stats:build && npm run stats:analyze",
        "stats:build": "npm run build -- --stats-json --source-map",
        "stats:analyze": "webpack-bundle-analyzer dist/angular-starter-app/stats.json",
        "lint": "ng lint",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --config=.prettierrc.json --loglevel=warn --write \"./src/**/*.(html|scss|ts)\"",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.12",
        "@angular/common": "~13.3.12",
        "@angular/compiler": "~13.3.12",
        "@angular/core": "~13.3.12",
        "@angular/forms": "~13.3.12",
        "@angular/platform-browser": "~13.3.12",
        "@angular/platform-browser-dynamic": "~13.3.12",
        "@angular/router": "~13.3.12",
        "@grapecity/wijmo.angular2.all": "^5.20222.877",
        "@syncfusion/ej2-angular-spreadsheet": "^21.2.3",
        "bootstrap": "^5.2.3",
        "jszip": "^3.10.1",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.11",
        "@angular-eslint/builder": "13.5.0",
        "@angular-eslint/eslint-plugin": "13.5.0",
        "@angular-eslint/eslint-plugin-template": "13.5.0",
        "@angular-eslint/schematics": "13.5.0",
        "@angular-eslint/template-parser": "13.5.0",
        "@angular/cli": "~13.3.11",
        "@angular/compiler-cli": "~13.3.12",
        "@fullstory/browser": "^1.7.0",
        "@moodys/blue-ng": "^1.12.3",
        "@moodys/blue-styles": "~1.9.0",
        "@moodys/emtn-ng": "^0.0.4-master.19",
        "@moodys/eslint-config": "^1.2.0",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "@okta/okta-angular": "^5.2.0",
        "@okta/okta-auth-js": "^6.7.6",
        "@popperjs/core": "^2.11.6",
        "@splitsoftware/splitio-angular": "^1.0.1",
        "@types/google.visualization": "0.0.66",
        "@types/jasmine": "~3.10.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "concurrently": "^7.3.0",
        "date-fns": "^2.29.3",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-jasmine": "^4.1.3",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-rxjs": "^5.0.2",
        "husky": "^8.0.1",
        "jasmine-core": "^4.0.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.3.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "^2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.7.0",
        "live-server": "^1.2.2",
        "lodash-es": "^4.17.21",
        "prettier": "^2.8.4",
        "ts-node": "~8.3.0",
        "typescript": "~4.6.4",
        "uid": "^1.0.0",
        "webpack-bundle-analyzer": "^4.5.0"
    }
}
